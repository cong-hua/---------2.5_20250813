{
  "name": "xiaohongshu-plugin-backend-fixed",
  "version": "1.0.0",
  "description": "小红书自动发布插件后端API服务 - 修复版",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "init-db": "node scripts/init-mongodb.js",
    "setup": "node scripts/setup-payment.js",
    "quick-setup": "node scripts/quick-setup.js",
    "setup-all": "npm run quick-setup && npm install && npm run init-db -- --with-test-user",
    "test-config": "node scripts/test-config.js",
    "postinstall": "npm rebuild bcryptjs"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "xiaohongshu",
    "automation",
    "points",
    "payment",
    "api"
  ],
